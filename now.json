{
    "version": 2,
    "routes": [
        { "src": "/api/.*", "dest": "/api/now.js" },
        { "src": "/.*", "dest": "/api/now.js" }
    ],
  "routes": [
    {
      "src": "/users",
      "dest": "/app/index.js",
      "methods": [
        "GET"
      ]
    },
    {
      "src": "/user",
      "dest": "/app/index.js",
      "methods": [
        "POST"
      ]
    }
  ]
}
